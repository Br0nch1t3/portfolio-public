---
type Props = {
  length?: number;
};

const props = Astro.props;

const length = props.length ?? 20;
---

<div class="hidden flex-col space-y-2 text-red-500 lg:flex">
  {
    Array(length)
      .fill(0)
      .map((_, i) => (
        <span class="text-2xl xl:text-3xl text-[#6271a4]">{10 + i}</span>
      ))
  }
</div>
